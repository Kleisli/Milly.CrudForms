prototype(Milly.Crudforms:Helper.CssClassRenderer) < prototype(Neos.Fusion:Component) {

    classPath = ''

    renderer = ${Configuration.setting('Milly.CrudForms.themes.' + millyCrudTheme + '.' + props.classPath)}

    @cache {
        mode = 'cached'
        entryIdentifier {
            path = ${'Milly.CrudForms.themes.' + millyCrudTheme + '.' + props.classPath}
        }
    }

}
